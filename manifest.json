{
    "manifest_version": 3,
    "name": "WolfNet+",
    "version": "1.0",
    "description": "Injects modern CSS in the style of WolfKey into WolfNet.",
    "permissions": ["activeTab", "scripting"],
    "content_scripts": [
      {
        "matches": ["https://wpga.myschoolapp.com/*"],
        "js": ["scrollNav.js","rearrangeNav.js", "removeTeacher.js", "rearrangeProgress.js", "siteInfo.js", "loadCSS.js", "assignmentCenterColor.js", "rearrangeAssignmentDetail.js"],
        "css": [
            "content.css",
            "modules/navbar.css",
            "modules/cards.css",
            "modules/progressbar.css",
            "modules/calender.css",
            "modules/classdetail.css",
            "modules/progress.css",
            "modules/assignment-center.css",
            "modules/schedule.css",
            "modules/assignment-detail.css"
          ]
      }
    ],
    "icons": {
    }
  }
  